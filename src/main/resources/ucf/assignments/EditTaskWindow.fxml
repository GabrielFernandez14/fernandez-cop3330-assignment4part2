<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="51.0" prefWidth="513.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.ToDoListController">
   <children>
      <DatePicker fx:id="editDate" layoutX="7.0" layoutY="13.0" prefHeight="26.0" prefWidth="116.0" promptText="YYYY-MM-DD" />
      <TextField fx:id="editDescription" layoutX="132.0" layoutY="13.0" prefHeight="26.0" prefWidth="281.0" promptText="Enter New Description..." />
      <Button fx:id="confirmEditButton" layoutX="422.0" layoutY="13.0" mnemonicParsing="false" onAction="#confirmEditClicked" prefHeight="26.0" prefWidth="84.0" text="Confirm" />
   </children>
</AnchorPane>
